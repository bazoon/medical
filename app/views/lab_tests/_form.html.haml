=bootstrap_form_for [@client, @lab_test],:html => {:class =>"well form-horizontal"} do |f|
  =f.date_select :test_date,:label => "Дата" 
  =#f.collection_select :lab_test_type_id,Ref::LabTestType.all,:id,:name,:label => "Тип анализа"
  =f.text_field :lab_test_type_name,:label => "Тип анализа"
  =f.text_field :result,:label => "Результат"
  .form-actions
    =f.submit "Сохранить",:class => "btn primary"
