=form_tag path do |f|
  =f.text_field :start_date, :label => "Серия"
  =f.text_field :end_date, :label => "Номер"
  =render :partial => "form_helpers/date_tag",:locals => {:label => "Начальная дата", :field_name => "start_date", :hint => ""}
  =render :partial => "form_helpers/date_tag",:locals => {:label => "Конечная дата", :field_name => "end_date", :hint => ""}
  .form-actions
    =submit_tag "Сохранить",:class => "btn primary"
