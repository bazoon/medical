%h3 Структура численного состава
- present "PeopleStructure" do |p|
  - p.prepare(sd,ed,years)

  %table.condensed-table.zebra-striped

    %thead
      %tr
        %th Наименование
        -years.each do |k|
          %th{:colspan =>2}= k
      %tr
        %th 
        -years.each do |k|
          %th факт
          %th % 

    %tbody

      - {1 => "ИОВ",
         2 => "УОВ",
         3 => "Участники, вооруженных конфликтов",
         4 => "Члены семей погибших военнослужащих",
         5 => "Жители блокадного Ленинграда",
         6 => "Бывшие малолетние узники концлагерей",
         7 => "Участники трудового фронта",
         8 => "Репрессированные",
         9 => "Ликвидаторы аварии на ЧАЭС",
         10 => "Ветераны труда",
         11 => "Пенсионеры"}.each do |k,v|
        %tr
          %td
            =v
            -p.fill(p.years,k)
            -p.years.each do |year|
              %td= p.observed(year,k)
              %td=# p.rel_row_value(p.observed(year,k),year)
      %tr
        %td
          Всего
          -p.years.each do |year|
            %td= #p.totals_by_year[year]
            %td 100 %
